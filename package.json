{
  "name": "@lackadaisical/metadata-extractor",
  "version": "0.1.1",
  "engines": {
    "node": ">=14.0.0"
  },
  "description": "Extracts all yaml 'documents' within a file to an array of objects for further parsing.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": "https://github.com/lackadaisical-documentation/metadata-extractor",
  "author": "Matt Jolly <Matt.Jolly@footclan.ninja>",
  "license": "GPL-3.0-or-later",
  "private": false,
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/node": "^18.7.17",
    "ava": "^3.15.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ava": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^35.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "test": "nyc ava",
    "build": "tsc"
  },
  "ava": {
    "files": [
      "test/test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "type": "commonjs",
  "files": [
    "./dist/src/**"
  ],
  "dependencies": {
    "yaml": "^1.10.2"
  }
